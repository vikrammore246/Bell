<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bell">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logofinal"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logofinal"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MonFriTimings" />
        <activity android:name=".SatTimings" />
        <activity android:name=".NewTimings" />
        <activity android:name=".AnnouncementActivity"></activity>
    </application>

</manifest>